{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "url",
    "forks_url",
    "commits_url",
    "id",
    "node_id",
    "git_pull_url",
    "git_push_url",
    "html_url",
    "files",
    "public",
    "created_at",
    "updated_at",
    "description",
    "comments",
    "user",
    "comments_url",
    "owner",
    "truncated"
  ],
  "properties": {
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "The Url Schema",
      "default": "",
      "examples": [
        "https://api.github.com/gists/95377fe8b3fb1acbe888bffea0263e40"
      ],
      "pattern": "^(.*)$"
    },
    "forks_url": {
      "$id": "#/properties/forks_url",
      "type": "string",
      "title": "The Forks_url Schema",
      "default": "",
      "examples": [
        "https://api.github.com/gists/95377fe8b3fb1acbe888bffea0263e40/forks"
      ],
      "pattern": "^(.*)$"
    },
    "commits_url": {
      "$id": "#/properties/commits_url",
      "type": "string",
      "title": "The Commits_url Schema",
      "default": "",
      "examples": [
        "https://api.github.com/gists/95377fe8b3fb1acbe888bffea0263e40/commits"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "95377fe8b3fb1acbe888bffea0263e40"
      ],
      "pattern": "^(.*)$"
    },
    "node_id": {
      "$id": "#/properties/node_id",
      "type": "string",
      "title": "The Node_id Schema",
      "default": "",
      "examples": [
        "MDQ6R2lzdDk1Mzc3ZmU4YjNmYjFhY2JlODg4YmZmZWEwMjYzZTQw"
      ],
      "pattern": "^(.*)$"
    },
    "git_pull_url": {
      "$id": "#/properties/git_pull_url",
      "type": "string",
      "title": "The Git_pull_url Schema",
      "default": "",
      "examples": [
        "https://gist.github.com/95377fe8b3fb1acbe888bffea0263e40.git"
      ],
      "pattern": "^(.*)$"
    },
    "git_push_url": {
      "$id": "#/properties/git_push_url",
      "type": "string",
      "title": "The Git_push_url Schema",
      "default": "",
      "examples": [
        "https://gist.github.com/95377fe8b3fb1acbe888bffea0263e40.git"
      ],
      "pattern": "^(.*)$"
    },
    "html_url": {
      "$id": "#/properties/html_url",
      "type": "string",
      "title": "The Html_url Schema",
      "default": "",
      "examples": [
        "https://gist.github.com/95377fe8b3fb1acbe888bffea0263e40"
      ],
      "pattern": "^(.*)$"
    },
    "files": {
      "$id": "#/properties/files",
      "type": "object",
      "title": "The Files Schema",
      "required": [
        "be_md.txt"
      ],
      "properties": {
        "be_md.txt": {
          "$id": "#/properties/files/properties/be_md.txt",
          "type": "object",
          "title": "The Be_md.txt Schema",
          "required": [
            "filename",
            "type",
            "language",
            "raw_url",
            "size"
          ],
          "properties": {
            "filename": {
              "$id": "#/properties/files/properties/be_md.txt/properties/filename",
              "type": "string",
              "title": "The Filename Schema",
              "default": "",
              "examples": [
                "be_md.txt"
              ],
              "pattern": "^(.*)$"
            },
            "type": {
              "$id": "#/properties/files/properties/be_md.txt/properties/type",
              "type": "string",
              "title": "The Type Schema",
              "default": "",
              "examples": [
                "text/plain"
              ],
              "pattern": "^(.*)$"
            },
            "language": {
              "$id": "#/properties/files/properties/be_md.txt/properties/language",
              "type": "string",
              "title": "The Language Schema",
              "default": "",
              "examples": [
                "Text"
              ],
              "pattern": "^(.*)$"
            },
            "raw_url": {
              "$id": "#/properties/files/properties/be_md.txt/properties/raw_url",
              "type": "string",
              "title": "The Raw_url Schema",
              "default": "",
              "examples": [
                "https://gist.githubusercontent.com/nichitapavel/95377fe8b3fb1acbe888bffea0263e40/raw/5aa0d95b78a99ed1fc11164912641f5131522f58/be_md.txt"
              ],
              "pattern": "^(.*)$"
            },
            "size": {
              "$id": "#/properties/files/properties/be_md.txt/properties/size",
              "type": "integer",
              "title": "The Size Schema",
              "default": 0,
              "examples": [
                122
              ]
            }
          }
        }
      }
    },
    "public": {
      "$id": "#/properties/public",
      "type": "boolean",
      "title": "The Public Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2018-10-22T23:34:33Z"
      ],
      "pattern": "^(.*)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2018-10-22T23:34:33Z"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "be-md"
      ],
      "pattern": "^(.*)$"
    },
    "comments": {
      "$id": "#/properties/comments",
      "type": "integer",
      "title": "The Comments Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "user": {
      "$id": "#/properties/user",
      "type": "null",
      "title": "The User Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "comments_url": {
      "$id": "#/properties/comments_url",
      "type": "string",
      "title": "The Comments_url Schema",
      "default": "",
      "examples": [
        "https://api.github.com/gists/95377fe8b3fb1acbe888bffea0263e40/comments"
      ],
      "pattern": "^(.*)$"
    },
    "owner": {
      "$id": "#/properties/owner",
      "type": "object",
      "title": "The Owner Schema",
      "required": [
        "login",
        "id",
        "node_id",
        "avatar_url",
        "gravatar_id",
        "url",
        "html_url",
        "followers_url",
        "following_url",
        "gists_url",
        "starred_url",
        "subscriptions_url",
        "organizations_url",
        "repos_url",
        "events_url",
        "received_events_url",
        "type",
        "site_admin"
      ],
      "properties": {
        "login": {
          "$id": "#/properties/owner/properties/login",
          "type": "string",
          "title": "The Login Schema",
          "default": "",
          "examples": [
            "nichitapavel"
          ],
          "pattern": "^(.*)$"
        },
        "id": {
          "$id": "#/properties/owner/properties/id",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            2392793
          ]
        },
        "node_id": {
          "$id": "#/properties/owner/properties/node_id",
          "type": "string",
          "title": "The Node_id Schema",
          "default": "",
          "examples": [
            "MDQ6VXNlcjIzOTI3OTM="
          ],
          "pattern": "^(.*)$"
        },
        "avatar_url": {
          "$id": "#/properties/owner/properties/avatar_url",
          "type": "string",
          "title": "The Avatar_url Schema",
          "default": "",
          "examples": [
            "https://avatars1.githubusercontent.com/u/2392793?v=4"
          ],
          "pattern": "^(.*)$"
        },
        "gravatar_id": {
          "$id": "#/properties/owner/properties/gravatar_id",
          "type": "string",
          "title": "The Gravatar_id Schema",
          "default": "",
          "examples": [
            ""
          ],
          "pattern": "^(.*)$"
        },
        "url": {
          "$id": "#/properties/owner/properties/url",
          "type": "string",
          "title": "The Url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel"
          ],
          "pattern": "^(.*)$"
        },
        "html_url": {
          "$id": "#/properties/owner/properties/html_url",
          "type": "string",
          "title": "The Html_url Schema",
          "default": "",
          "examples": [
            "https://github.com/nichitapavel"
          ],
          "pattern": "^(.*)$"
        },
        "followers_url": {
          "$id": "#/properties/owner/properties/followers_url",
          "type": "string",
          "title": "The Followers_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/followers"
          ],
          "pattern": "^(.*)$"
        },
        "following_url": {
          "$id": "#/properties/owner/properties/following_url",
          "type": "string",
          "title": "The Following_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/following{/other_user}"
          ],
          "pattern": "^(.*)$"
        },
        "gists_url": {
          "$id": "#/properties/owner/properties/gists_url",
          "type": "string",
          "title": "The Gists_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/gists{/gist_id}"
          ],
          "pattern": "^(.*)$"
        },
        "starred_url": {
          "$id": "#/properties/owner/properties/starred_url",
          "type": "string",
          "title": "The Starred_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/starred{/owner}{/repo}"
          ],
          "pattern": "^(.*)$"
        },
        "subscriptions_url": {
          "$id": "#/properties/owner/properties/subscriptions_url",
          "type": "string",
          "title": "The Subscriptions_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/subscriptions"
          ],
          "pattern": "^(.*)$"
        },
        "organizations_url": {
          "$id": "#/properties/owner/properties/organizations_url",
          "type": "string",
          "title": "The Organizations_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/orgs"
          ],
          "pattern": "^(.*)$"
        },
        "repos_url": {
          "$id": "#/properties/owner/properties/repos_url",
          "type": "string",
          "title": "The Repos_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/repos"
          ],
          "pattern": "^(.*)$"
        },
        "events_url": {
          "$id": "#/properties/owner/properties/events_url",
          "type": "string",
          "title": "The Events_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/events{/privacy}"
          ],
          "pattern": "^(.*)$"
        },
        "received_events_url": {
          "$id": "#/properties/owner/properties/received_events_url",
          "type": "string",
          "title": "The Received_events_url Schema",
          "default": "",
          "examples": [
            "https://api.github.com/users/nichitapavel/received_events"
          ],
          "pattern": "^(.*)$"
        },
        "type": {
          "$id": "#/properties/owner/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "User"
          ],
          "pattern": "^(.*)$"
        },
        "site_admin": {
          "$id": "#/properties/owner/properties/site_admin",
          "type": "boolean",
          "title": "The Site_admin Schema",
          "default": false,
          "examples": [
            false
          ]
        }
      }
    },
    "truncated": {
      "$id": "#/properties/truncated",
      "type": "boolean",
      "title": "The Truncated Schema",
      "default": false,
      "examples": [
        false
      ]
    }
  }
}
